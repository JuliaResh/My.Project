<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="759c210a-a170-4e04-b65a-21b0dea68ac0" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Version 3.0</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_459" name="External file" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_minVersion" value="3.0" />
          <param name="jetbrains_powershell_script_code" value="Write-Output $host" />
          <param name="jetbrains_powershell_script_file" value="host.ps1" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_460" name="External file with arguments" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_minVersion" value="3.0" />
          <param name="jetbrains_powershell_scriptArguments" value="arg1 arg2 arg3" />
          <param name="jetbrains_powershell_script_code" value="Write-Output $host" />
          <param name="jetbrains_powershell_script_file" value="host.ps1" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_461" name="Source code" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x64" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_minVersion" value="3.0" />
          <param name="jetbrains_powershell_script_code"><![CDATA[$PSVersionTable
If ([System.IntPtr]::Size -eq 4) { "32-bit" } ElseIf ([System.IntPtr]::Size -eq 8) { "64-bit" } Else {"smth wrong"}]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Powershell_PowerShell" />
    </vcs-settings>
    <requirements>
      <contains name="teamcity.agent.jvm.os.arch" value="64" />
    </requirements>
    <build-triggers />
    <build-extensions>
      <extension id="BUILD_EXT_108" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="matchesRegex" />
          <param name="buildFailureOnMessage.messagePattern" value="(powershell.exe|Arguments).{1,5}-Version\,? 3\.0" />
          <param name="buildFailureOnMessage.outputText" value="version 3.0 was not specified when launching powershell" />
          <param name="buildFailureOnMessage.reverse" value="true" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_109" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="matchesRegex" />
          <param name="buildFailureOnMessage.messagePattern" value="(powershell\.exe -Version 3\.0 .* arg1 arg2 arg3)|(Arguments.{3,5}-Version\, 3\.0)" />
          <param name="buildFailureOnMessage.reverse" value="true" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_110" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="32-bit" />
          <param name="buildFailureOnMessage.outputText" value="failed to launch 64-bit PowerShell" />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

